

class Main inherits IO {
   m: Int;
   n: Int;

   ackermann(m: Int, n: Int) : Int {
      if (m=0) then
         n+1
      else
         if (n=0) then
            ackermann(m-1, 1)
         else
            ackermann(m-1, ackermann(m, n-1))
         fi
      fi
   };

   main(): SELF_TYPE {
      {
         out_string("Por favor ingrese m: ");
         m <- in_int();
         out_string("Por favor ingrese n: ");
         n <- in_int();
         out_string("La funciÃ³n de ackerman para (");
         out_int(m);
         out_string(",");
         out_int(n);
         out_string(") es: ");
         out_int(ackermann(m,n));
         out_string("\n");
      }
   };
};
