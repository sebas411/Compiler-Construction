class Main {
  x : Int;
  y : Bool;
  z : String;
  main() : Int {
    {
      x <- 4;
      y <- true;
      z <- "Hola";

      -- Operaciones matematicas
      x <- x + 3;
      x <- 2 * 3;
      x <- 5 / x;
      x <- 8 - 3;
      
      -- Operaciones booleanas
      y <- 3 < 5;
      y <- 8 <= x;
      y <- 4 = 4;
      y <- true < false;
      y <- true <= y;
      y <- y = false;
      y <- "abcde" < "abcdf";
      y <- "abcdf" <= z;
      y <- z = "hola";

      -- Not y ~
      y <- not true;
      x <- ~7;
      y <- ~false;
    }
  };
};